{
  "expo": {
    "name": "family-command-center-app",
    "slug": "family-command-center-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/images/icon.png",
    "scheme": "familycommandcenterapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./app/assets/images/FamilyCommandCenterSplash.png",
      "resizeMode": "cover",
      "backgroundColor": "#0b1632"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/images/icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.familycommandcenterapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./app/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "expo-splash-screen",
        {
          "preventAutoHide": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
